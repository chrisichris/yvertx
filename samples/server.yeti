module server;

load yvertx;

https = load yvertx.https;

verticle do {vertx, container}:
    (https.createWithHandler do req:
        req#response#end("Hello World Yeti!");
    done vertx)#listen(8080, "localhost");
     
    (https.createWithHandler do req:
        req#response#end("Hello World Yeti!");
     done vertx)#listen(8090, "localhost");
     
     \()
done;
    