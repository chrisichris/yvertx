module deploy;

load yvertx;

verticle do :
    println "Deploying module";
    deployModule "org.foo.MyMod-v1.0" 
                    {``some-var`` = "hello", for_json = N()} 
                    1 
                    do deploymentId:
            println "This gets called when deployment is complete, deployment id is \(deploymentId)";
        done;
    \();
done;

